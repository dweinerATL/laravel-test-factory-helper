{
    "name": "mpociot/laravel-test-factory-helper",
    "description": "Generate Laravel test factories from your existing models",
    "keywords": ["Laravel", "Tests", "Factory"],
    "license": "MIT",
    "require": {
        "php": ">=7.0",
        "illuminate/support": "^5.5",
        "illuminate/console": "^5.5",
        "illuminate/filesystem": "^5.5",
        "doctrine/dbal": "~2.3",
        "phpdocumentor/reflection-docblock": "^4.3",
        "dweineratl/laravel-model-abstraction-helper": "dev-develop"
    },
    "autoload": {
        "psr-4": {
            "Mpociot\\LaravelTestFactoryHelper\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mpociot\\LaravelTestFactoryHelper\\TestFactoryHelperServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "git",
            "url": "git@github.com:dweinerATL/laravel-model-abstraction-helper.git"
        }
    ]
}
